\begin_layout Lemma
Sea 
\begin_inset Formula $G\in\Gamma_{n}$
\end_inset

 un grafo arbitrario y sean 
\begin_inset Formula $i\in G$
\end_inset

 un nodo y 
\begin_inset Formula $G(i)=G\cap(V(i)\times V(i))$
\end_inset

 el subgrafo de 
\begin_inset Formula $G$
\end_inset

 que contiene todos los vecinos de 
\begin_inset Formula $i$
\end_inset

, pero no 
\begin_inset Formula $i$
\end_inset

, y todas las aristas entre ellos.
 Supongamos que 
\begin_inset Formula $G(i)$
\end_inset

 se puede particionar en 
\begin_inset Formula $l$
\end_inset

 subgrafos completos disjuntos.
 Entonces, se cumple
\begin_inset Formula 
\[
c_{i}\geq\frac{1}{l}-\frac{1}{h},
\]

\end_inset


\end_layout

\begin_layout Lemma
donde 
\begin_inset Formula $h$
\end_inset

 es el número de nodos en el subgrafo más pequeño de 
\begin_inset Formula $G(i)$
\end_inset

.
\end_layout

\begin_layout Proof
Consideremos tal partición y pongamos 
\begin_inset Formula $q_{1},q_{2},...,q_{l}$
\end_inset

 para el número de nodos en cada subgrafo completo de 
\begin_inset Formula $G(i)$
\end_inset

.
 Tenemos 
\begin_inset Formula $a=\underset{j=1}{\overset{l}{\sum}}\frac{q_{j}(q_{j}-1)}{2}$
\end_inset

 aristas en 
\begin_inset Formula $G(i)$
\end_inset

, mientras que el número total de posibles aristas es 
\begin_inset Formula $t=\frac{(\underset{j=1}{\overset{l}{\sum}}q_{j})(-1+\underset{j=1}{\overset{l}{\sum}}q_{j})}{2}$
\end_inset

.
\end_layout

\begin_layout Proof
Luego,
\begin_inset Formula 
\[
c_{i}\geq\frac{a}{t}\geq\frac{\underset{j}{\sum}q_{j}(q_{j}-1)}{(\underset{j}{\sum}q_{j})^{2}}\geq\overset{\mathcal{A}}{\overbrace{\frac{\underset{j}{\sum}q_{j}^{2}}{(\underset{j}{\sum}q_{j})^{2}}}}-\overset{\mathcal{B}}{\overbrace{\frac{\underset{j}{\sum}q_{j}}{\underset{j}{\sum}q_{j}^{2}}}}.
\]

\end_inset


\end_layout

\begin_layout Proof
Tenemos para 
\begin_inset Formula $\mathcal{A}$
\end_inset

, usando 
\begin_inset Formula $2q_{j}q_{k}\leq q_{k}^{2}+q_{j}^{2}$
\end_inset

,
\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
(\underset{j}{\sum}q_{j})^{2}\leq\underset{j}{\sum}q_{j}^{2}+\frac{1}{2}\underset{k\neq j}{\overset{l}{\sum}}(q_{k}^{2}+q_{j}^{2})=\underset{j}{\sum}q_{j}^{2}+\frac{1}{2}\underset{k\neq j}{\overset{l}{\sum}}q_{k}^{2}+\frac{1}{2}\underset{k\neq j}{\overset{l}{\sum}}q_{j}^{2}
\]

\end_inset


\begin_inset Formula 
\[
(\underset{j}{\sum}q_{j})^{2}\leq\underset{j}{\sum}q_{j}^{2}+(l-1)\underset{j}{\sum}q_{j}^{2}=l\underset{j}{\sum}q_{j}^{2}.
\]

\end_inset


\end_layout

\begin_layout Proof
Es decir, 
\begin_inset Formula $\mathcal{A}\geq\frac{1}{l}$
\end_inset

.
\end_layout

\begin_layout Proof
Por otro lado, notamos que 
\begin_inset Formula $\underset{j}{\sum}hq_{j}\leq\underset{j}{\sum}q_{j}^{2}$
\end_inset

, ó 
\begin_inset Formula $\mathcal{B}\leq\frac{1}{h}$
\end_inset

.
\end_layout

\begin_layout Proof
Finalmente, 
\begin_inset Formula $c_{i}\geq\frac{1}{l}-\frac{1}{h}$
\end_inset

.
\end_layout

\begin_layout Lemma
Sean 
\begin_inset Formula $f(n)$
\end_inset

, 
\begin_inset Formula $g(n)$
\end_inset

 dos funciones tal que 
\begin_inset Formula $\underset{n\to\infty}{\lim}f(n)=\underset{n\to\infty}{\lim}g(n)=\infty$
\end_inset

, donde 
\begin_inset Formula $g(n)$
\end_inset

 es no decreciente.
 Supongamos que 
\begin_inset Formula $\underset{n\to\infty}{\lim}h(n)=\frac{f(n)}{g(n)}=0$
\end_inset

.
 Entonces, existe un 
\begin_inset Formula $n_{0}$
\end_inset

 tal que
\begin_inset Formula 
\[
\forall n\geq n_{0}:\,(1-\frac{1}{f(n)})^{g(n)}\geq1-\frac{g(n)}{f(n)}.
\]

\end_inset


\end_layout

\begin_layout Proof
Dado, 
\begin_inset Formula $i\in\mathbb{N}$
\end_inset

, tenemos 
\begin_inset Formula $g(n)^{i}\geq g(n)$
\end_inset

 y además,
\begin_inset Formula 
\[
\frac{-g(n)}{i\, f(n)^{i}}\geq\frac{-g(n)^{i}}{i\, f(n)^{i}}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
g(n)\underset{i=1}{\overset{\infty}{\sum}}\frac{(-1)^{i+1}}{i}\big(\frac{-1}{f(n)}\big)^{i}\geq\underset{i=1}{\overset{\infty}{\sum}}\frac{(-1)^{i+1}}{i}\big(\frac{-g(n)}{f(n)}\big)^{i}.
\]

\end_inset


\end_layout

\begin_layout Proof
Para algún 
\begin_inset Formula $n_{0}$
\end_inset

, tendremos 
\begin_inset Formula $f(n)>g(n)>1$
\end_inset

.
 Luego,
\begin_inset Formula 
\[
g(n)\log(1-\frac{1}{f(n)})\geq\log(1-\frac{g(n)}{f(n)})
\]

\end_inset


\begin_inset Formula 
\[
(1-\frac{1}{f(n)})^{g(n)}\ge1-\frac{g(n)}{f(n)}
\]

\end_inset


\end_layout

